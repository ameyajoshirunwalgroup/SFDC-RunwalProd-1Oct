          <b> Error Details - {!v.errorMessage} </b>
        </aura:if>
         </div>
        <aura:set attribute="else">
       
             <!-- <aura:if isTrue="{!and (not(empty(v.encResp)),v.isPaymentSuccess == 'Success') }"> -->
             <aura:if isTrue="{!and (or(not(empty(v.encResp)),not(empty(v.msg))),v.isPaymentSuccess == 'Success')}"> <!-- bkk -->
        <div class="slds-p-around_small slds-text-heading_small slds-align_absolute-center">
            <div class="slds-text-color_success"><label>Thank You ! Your EOI Submission and Payment is successful . Your EOI Reference Number is <b>{!v.EOIWrap.RWName}</b> </label> </div>
        </div>
            <aura:set attribute="else">
				<!-- <aura:if isTrue="{!and (not(empty(v.encResp)),v.isPaymentFailure == 'Failure') }"> -->
                <aura:if isTrue="{!and (or(not(empty(v.encResp)),not(empty(v.msg))),v.isPaymentFailure == 'Failure')}"> <!-- bkk -->
