public class ObjectRecordCountScheduler implements Schedulable {

    public void execute(SchedulableContext sc) {
        Map<String, Schema.SObjectType> gd = Schema.getGlobalDescribe();

        // Convert keySet() (which is a Set<String>) into a List<String>
        List<String> allObjects = new List<String>();
        allObjects.addAll(gd.keySet());
        allObjects.sort(); // optional for predictable order

        Integer batchSize = 50;
        Integer total = allObjects.size();

        // Manually create subsets since Apex doesn't support subList()
        for (Integer i = 0; i < total; i += batchSize) {
            List<String> subset = new List<String>();

            for (Integer j = i; j < Math.min(i + batchSize, total); j++) {
                subset.add(allObjects[j]);
            }

            System.enqueueJob(new ObjectRecordCountQueueable(subset));
        }
    }
