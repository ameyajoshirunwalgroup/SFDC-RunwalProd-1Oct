    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Add_Team_Member</name>
        <label>Add Team Members</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>TM_Records</assignToReference>
            <operator>Add</operator>
            <value>
                <elementReference>Loop_AllMembers</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Loop_AllMembers</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Is_this_member_s_Approver_Type_in_the_list</name>
        <label>Is this memberâ€™s Approver Type in the list?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Loop_AllMembers</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>is_True</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ApproverTypes</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>FormulaToConvertApproverTypeintoText</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Add_Team_Member</targetReference>
            </connector>
            <label>is True</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>FormulaToConvertApproverTypeintoText</name>
        <dataType>String</dataType>
        <expression>TEXT({!Loop_AllMembers.Approver_Type__c})</expression>
    </formulas>
    <interviewLabel>Fetch Project Team_Members {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Fetch Project Team_Members</label>
    <loops>
        <name>Loop_AllMembers</name>
        <label>Loop_AllMembers</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <collectionReference>get_CP_Invoice_Team_Members</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Is_this_member_s_Approver_Type_in_the_list</targetReference>
        </nextValueConnector>
    </loops>
