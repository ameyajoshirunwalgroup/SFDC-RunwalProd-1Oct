                <leftValueReference>$Record.RW_Signed_Agreement__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Not yet shared</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RW_Signed_Agreement_to_Customer_Date__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>fiveDaysFromRegistrationDate</elementReference>
                </rightValue>
            </conditions>
