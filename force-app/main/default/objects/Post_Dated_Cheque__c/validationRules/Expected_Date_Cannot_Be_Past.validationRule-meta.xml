<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Expected_Date_Cannot_Be_Past</fullName>
    <active>true</active>
    <errorConditionFormula>Expected_Clearance_Date__c  &lt; Today() || Expected_Clearance_Date__c &lt;  Cheque_Receipt_Date__c</errorConditionFormula>
    <errorDisplayField>Expected_Clearance_Date__c</errorDisplayField>
    <errorMessage>Expected Clearance Date cannot be in the past and can not be before cheque received date.</errorMessage>
</ValidationRule>
