<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cheque_Amt_Greater_Than_Demand_Amt</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Cheque_Amount__c  &gt;  Demand__r.RW_Total_Demand_Amount__c )</errorConditionFormula>
    <errorDisplayField>Cheque_Amount__c</errorDisplayField>
    <errorMessage>Cheque Amount cannot be greater than Demand Amount.</errorMessage>
</ValidationRule>
