<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Billing_City_Max_40_Chars</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISNEW(),LEN(BillingCity) &gt; 40)</errorConditionFormula>
    <errorDisplayField>BillingCity</errorDisplayField>
    <errorMessage>Billing City cannot be more than 40 characters.</errorMessage>
</ValidationRule>
