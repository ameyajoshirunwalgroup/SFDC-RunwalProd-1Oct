<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Billing_Street_Max_60_Chars</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISNEW(),LEN(BillingStreet) &gt; 60)</errorConditionFormula>
    <errorDisplayField>BillingStreet</errorDisplayField>
    <errorMessage>Billing Street cannot be more than 60 characters.</errorMessage>
</ValidationRule>
