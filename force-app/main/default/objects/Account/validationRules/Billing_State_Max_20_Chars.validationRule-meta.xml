<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Billing_State_Max_20_Chars</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISNEW(),LEN(BillingState) &gt; 20)</errorConditionFormula>
    <errorDisplayField>BillingState</errorDisplayField>
    <errorMessage>Billing State cannot be more than 20 characters.</errorMessage>
</ValidationRule>
