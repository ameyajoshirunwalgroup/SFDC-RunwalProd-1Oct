<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Billing_PostalCode_Max_10_Chars</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISNEW(),LEN(BillingPostalCode) &gt; 10)</errorConditionFormula>
    <errorDisplayField>BillingPostalCode</errorDisplayField>
    <errorMessage>Billing State cannot be more than 10 characters.</errorMessage>
</ValidationRule>
