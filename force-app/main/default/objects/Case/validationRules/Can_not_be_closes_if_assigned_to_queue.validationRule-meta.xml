<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Can_not_be_closes_if_assigned_to_queue</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    Owner:Queue.Id != null,
   ISPICKVAL(Status, &apos;Case Closed&apos;) 				
)</errorConditionFormula>
    <errorMessage>You can not close the case which is assigned to Queue. Please change the owner to close the Case.</errorMessage>
</ValidationRule>
