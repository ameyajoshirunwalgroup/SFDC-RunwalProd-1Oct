<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Missed_Call_ageing_in_Hours__c</fullName>
    <externalId>false</externalId>
    <formula>IF(Last_CRM_Call_Subject__c = &apos;Missed Call&apos;, (NOW() - Last_CRM_Call_Attempt_Date__c) * 24,NULL)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Missed Call ageing in Hours</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
