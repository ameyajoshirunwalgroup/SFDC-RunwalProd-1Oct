<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Channel_partner_on_campaign</fullName>
    <active>false</active>
    <errorConditionFormula>AND(!ISBLANK(CampaignId), ISPICKVAL(LeadSource, &quot;Channel Partner&quot;))</errorConditionFormula>
    <errorMessage>You cannot enter this lead source as campaign is selected</errorMessage>
</ValidationRule>
