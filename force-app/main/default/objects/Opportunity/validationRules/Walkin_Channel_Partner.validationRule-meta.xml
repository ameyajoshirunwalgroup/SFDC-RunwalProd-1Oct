<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Walkin_Channel_Partner</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL( Walkin_Source__c , &apos;Channel Partner&apos;), ISBLANK( RW_Walkin_Channel_Partner__c ))</errorConditionFormula>
    <errorDisplayField>RW_Walkin_Channel_Partner__c</errorDisplayField>
    <errorMessage>Please enter the Walkin Channel Partner when Walkin source is Channel Partner.</errorMessage>
</ValidationRule>
