<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_no_of_chang_in_walkinSourc</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
!ISBLANK( Walk_in_Source_change__c ), 
ISCHANGED( Walkin_Source__c ),$Profile.Name &lt;&gt; &quot;System Administrator&quot; )</errorConditionFormula>
    <errorDisplayField>Walkin_Source__c</errorDisplayField>
    <errorMessage>You cannot change the walkin source.</errorMessage>
</ValidationRule>
