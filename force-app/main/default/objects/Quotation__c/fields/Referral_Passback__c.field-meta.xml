<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Referral_Passback__c</fullName>
    <formula>ROUND(IF(Discount_10_L__c &gt; 0,Discount_10_L__c / Agreement_Value_D__c, 0),4)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>If Referral Passback% is greater than 1% then prevent submit for approval.</inlineHelpText>
    <label>Referral Passback%</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
