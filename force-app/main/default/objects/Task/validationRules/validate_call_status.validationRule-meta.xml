<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>validate_call_status</fullName>
    <active>true</active>
    <errorConditionFormula>AND(  ISPICKVAL(Call_Status__c, &quot;Visit Confirmed&quot;), ISBLANK( RW_Call_Timing__c )   )</errorConditionFormula>
    <errorDisplayField>RW_Call_Timing__c</errorDisplayField>
    <errorMessage>Please enter the visit date</errorMessage>
</ValidationRule>
