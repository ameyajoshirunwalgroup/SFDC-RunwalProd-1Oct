<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>validate_pan_no</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT(ISBLANK( Broker_Pan_No__c )),NOT(REGEX(Broker_Pan_No__c ,&quot;[a-zA-Z0-9]{10}&quot;)))</errorConditionFormula>
    <errorDisplayField>Broker_Pan_No__c</errorDisplayField>
    <errorMessage>Please enter an valid pan number.</errorMessage>
</ValidationRule>
