<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>mobile_number</fullName>
    <active>false</active>
    <errorConditionFormula>AND(OR(IF(RW_Mobile_No__c!= null, NOT(REGEX(RW_Mobile_No__c, &quot;[0-9]{10,15}&quot;)), false)))</errorConditionFormula>
    <errorDisplayField>RW_Mobile_No__c</errorDisplayField>
    <errorMessage>Please enter a valid mobile number</errorMessage>
</ValidationRule>
