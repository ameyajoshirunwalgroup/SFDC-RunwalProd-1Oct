<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AOP_CP_is_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(DATEVALUE(CreatedDate)&gt;= DATE(2024,04,11),ISNULL(AOP_CP__c))</errorConditionFormula>
    <errorDisplayField>AOP_CP__c</errorDisplayField>
    <errorMessage>Please Enter the AOP CP% on the Brokerage Scheme.</errorMessage>
</ValidationRule>
