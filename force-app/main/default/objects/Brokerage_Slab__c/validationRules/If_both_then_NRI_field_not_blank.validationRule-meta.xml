<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>If_both_then_NRI_field_not_blank</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL(  Brokerage_Scheme__r.Type__c , &quot;Both&quot;) ,  ISNULL( Additional_Brokerage_for_OS_NRI__c ) )</errorConditionFormula>
    <errorMessage>Additional Brokerage For OS/NRI should not be blank</errorMessage>
</ValidationRule>
