<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cost_per_Response__c</fullName>
    <formula>IF(AND( NumberOfResponses  &lt;&gt; 0, NOT(ISNULL(NumberOfResponses))),  ActualCost /  NumberOfResponses , 0)</formula>
    <label>Cost per Lead</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Currency</type>
</CustomField>
