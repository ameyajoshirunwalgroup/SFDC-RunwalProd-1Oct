<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cost_per_Booked_Opportunities__c</fullName>
    <formula>IF(
AND(NumberOfWonOpportunities &lt;&gt; 0, NOT(ISNULL(NumberOfWonOpportunities))),
ActualCost /  NumberOfWonOpportunities , 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Campaign Spend per Won Opportunity</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Currency</type>
</CustomField>
