<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Campaign_Spend_per_Won_Opportunity__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
AND(NumberOfWonOpportunities &lt;&gt; 0, NOT(ISNULL(NumberOfWonOpportunities))),
ActualCost / NumberOfWonOpportunities , 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Campaign Spend per Won Opportunity</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
