<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>mandate_Toll_Free_Number</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISBLANK(Tollfree_Number__c),
    NOT(ISPICKVAL(Origin__c, &quot;Online&quot;)),
NOT(ISPICKVAL(Type__c, &quot;CP Meets&quot;))
)</errorConditionFormula>
    <errorMessage>Tollfree number is  mandatory,</errorMessage>
</ValidationRule>
