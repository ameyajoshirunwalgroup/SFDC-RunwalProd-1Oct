<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Campaign_Id_for_Anarock_is_mandatory</fullName>
    <active>true</active>
    <errorConditionFormula>AND( Send_Digital_data_to_Anarock__c , ISBLANK( Anarock_Campaign_Id__c ) )</errorConditionFormula>
