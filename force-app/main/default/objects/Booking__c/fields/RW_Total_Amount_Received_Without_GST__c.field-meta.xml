<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>RW_Total_Amount_Received_Without_GST__c</fullName>
    <formula>(RW_Total_Receipt_Amount_Received__c - ((RW_Total_Receipt_Amount_Received__c/RW_Total_Demand_Raised__c)*RW_Total_GST_Amount__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Amount Received Without GST</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
