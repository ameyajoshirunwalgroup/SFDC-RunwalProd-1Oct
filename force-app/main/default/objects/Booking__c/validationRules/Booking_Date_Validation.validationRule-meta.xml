<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Booking_Date_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
NOT(ISBLANK(Booking_Date__c)),
DATEVALUE(Booking_Date__c) &gt; TODAY()
)</errorConditionFormula>
    <errorMessage>Booking date should not be the future date</errorMessage>
</ValidationRule>
