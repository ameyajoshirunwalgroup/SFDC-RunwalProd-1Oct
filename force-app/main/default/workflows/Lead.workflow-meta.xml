    <rules>
        <fullName>Lead Source Modified Date</fullName>
        <actions>
            <name>Lead_source_Modified_date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>ISCHANGED(LeadSource)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Lead_Source_Loyalty</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Lead.LeadSource</field>
            <operation>equals</operation>
            <value>Loyalty</value>
        </criteriaItems>
        <description>If Loyalty is selected as lead source, Referral details are mandatory</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
