<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Sales_Manager</submitter>
        <type>role</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>RW_Primary_First_Name__c</field>
        <field>RW_Primary_Last_Name__c</field>
        <field>RW_Reason__c</field>
        <field>RW_Sub_Reason__c</field>
        <field>RW_Date_of_Cancellation__c</field>
        <field>RW_Primary_Email__c</field>
        <field>RW_Primary_Contact_No__c</field>
        <field>RW_Status__c</field>
        <field>Owner</field>
        <field>RW_Cancellation_Email_Link__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>RW_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Step 1</label>
        <name>Step_1</name>
    </approvalStep>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>RW_EOI__c.RW_Approval_Status__c</field>
            <operation>notEqual</operation>
            <value>Sent for Approval,Approved</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Approval_Status_Change</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Refund_Pending</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Approval_Rejection</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Approval_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Cancellation_Status</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>EOI Cancellation</label>
    <processOrder>1</processOrder>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
