    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>Cancellation_Requested_Status__c</field>
        <field>Cancellation_Reason__c</field>
        <field>Cancellation_Sub_reason__c</field>
        <field>Cancellation_Deed_Document_Link__c</field>
        <field>Cancellation_Letter_Link__c</field>
        <field>Cancellation_Checklist_Link__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Level_Two_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Booking__c.Level_Two_Approver__c</field>
                <operation>notEqual</operation>
                <value>null</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>Level One Approval</label>
        <name>Level_One_Approval</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>Cancellation_Notifiation_To_CRM_Head</name>
                <type>Alert</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Level_Three_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Second Level Approver</label>
        <name>Second_Level_Approver</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Booking__c.Cancellation_Requested_Status__c</field>
            <operation>equals</operation>
            <value>Cancellation Confirmed</value>
        </criteriaItems>
        <criteriaItems>
            <field>Booking__c.Cancellation_Reason__c</field>
            <operation>equals</operation>
            <value>Unit cancelled</value>
        </criteriaItems>
        <criteriaItems>
            <field>Booking__c.Status__c</field>
            <operation>equals</operation>
            <value>Cancellation Initiated</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Approval_Status</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Booking_cancelled</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Cancellation_approval_date</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Cancellation_Rejected</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Rejection_email_notification_to_RM</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Approval_Status_Update</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>Booking Cancellation 1_old</label>
    <processOrder>2</processOrder>
    <recallActions>
        <action>
            <name>Approval_Status_recalled</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>false</showApprovalHistory>
</ApprovalProcess>
