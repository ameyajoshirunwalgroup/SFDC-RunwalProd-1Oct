    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>Cancellation_Requested_Status__c</field>
        <field>Cancellation_Reason__c</field>
        <field>Cancellation_Sub_reason__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Level_Two_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Level One Approval</label>
        <name>Level_One_Approval</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>Cancellation_Notifiation_To_CRM_Head</name>
                <type>Alert</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Level_Three_Approver__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Second Level Apprrover</label>
        <name>Second_Level_Apprrover</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <emailTemplate>RW_Booking/RW_Booking_Cancellation</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Booking__c.Cancellation_Requested_Status__c</field>
            <operation>equals</operation>
            <value>Cancellation Confirmed</value>
        </criteriaItems>
        <criteriaItems>
            <field>Booking__c.Cancellation_Reason__c</field>
            <operation>equals</operation>
            <value>Unit transfer</value>
        </criteriaItems>
        <criteriaItems>
            <field>Booking__c.Status__c</field>
            <operation>equals</operation>
            <value>Cancellation Initiated</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
