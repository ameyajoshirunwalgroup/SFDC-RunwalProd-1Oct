            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <booleanFilter>1 OR 2</booleanFilter>
            <criteriaItems>
                <field>Booking__c.Waiver_Approver_Level__c</field>
                <operation>equals</operation>
                <value>Level 2</value>
            </criteriaItems>
            <criteriaItems>
                <field>Booking__c.Waiver_Approver_Level__c</field>
                <operation>equals</operation>
                <value>Level 3</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>CRM Head Approval</label>
        <name>Step_2</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>CEO__c</name>
