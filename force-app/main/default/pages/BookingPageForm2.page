                    <apex:OutputField value="{!boookingObj.Account_Name_Service_Tax__c}"  />
                    <apex:OutputField value="{!boookingObj.Account_Number__c}"   />
                    <apex:OutputField value="{!boookingObj.Account_Number_Service_tax__c}" />
                    <apex:OutputField value="{!boookingObj.Bank__c}" />
                    <apex:OutputField value="{!boookingObj.Bank_Name_Service_tax__c}" />
                    <apex:OutputField value="{!boookingObj.IFSC_Code_Flat_Cost__c}" />
                    <apex:OutputField value="{!boookingObj.IFSC_Code_Service_Tax__c}" />
                </apex:pageBlockSection>
                
                <apex:pageBlockSection title="Other Details" id="otherdetails" >
