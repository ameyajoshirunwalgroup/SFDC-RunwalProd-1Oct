                    <!-- <apex:OutputField id="bookingdate" value="{!boookingObj.Booking_Date__c}" /> OLD -->
                    <!-- <apex:inputField id="bookingDate" value="{!boookingObj.Booking_Date__c}" required="true"/> --><!-- Added By Netra on 28/12/16 -->
                    <apex:OutputField id="bookingdate1" value="{!boookingObj.Booking_Date__c}" rendered="{!disabelBookingDate == true}"/> <!-- Added By coServe 05-06-2024 -->
                    <apex:inputField id="bookingDate" value="{!boookingObj.Booking_Date__c}" label="Booking Date *"  rendered="{!disabelBookingDate == false}"/> <!-- Added By coServe 05-06-2024 -->
                    
                    <apex:OutputField value="{!boookingObj.Sales_Managers__c}" />
                    <apex:OutputField value="{!boookingObj.Sourcing_Mananger__c}"  />
                   <!-- <apex:outputText value="{!boookingObj.Brokerage__c}" label="Brokerage %   *" rendered="{!dupflag}"/> -->  <!--Commented by Sheetal on 1/2/2023-->
                    <apex:outputText value="{!boookingObj.Broker_Name__c}" styleclass="form-control" html-disabled="true"  rendered="{!dupflag1}"  />    <!--Added by Sheetal on 29/11/2021-->
                    <apex:outputText value="{!boookingObj.Emp_Manager_Name__c}" styleclass="form-control" html-disabled="true"  rendered="{!isEmpManager}"  />    <!--Added by Sheetal on 28/02/2022-->
                    <apex:outputText value="{!boookingObj.Customer_Reference__c}" styleclass="form-control" html-disabled="true"  rendered="{!isCustomerReference}"  /> 

                    <apex:outputText ></apex:outputText>
                </apex:pageBlockSection>
