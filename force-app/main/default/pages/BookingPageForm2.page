                    <!-- <apex:commandButton value="SAVE" action="{!Save}" Rerender="messages"  status="status"/>   --> <!-- Commented by Vinay 05-05-2025 -->
                    <!-- Added by Vinay 05-05-2025 Start -->
                    <apex:actionstatus id="idActionStatus">
                        <apex:facet name="stop">
                            <apex:commandButton value="Save" action="{!Save}" status="idActionStatus"  disabled="false" rerender="idActionStatus,messages"/>
                        </apex:facet>
                        <apex:facet name="start">
                            <apex:commandButton value="Saving..." action="{!Save}" status="idActionStatus"  disabled="true"/>
                        </apex:facet>
                    </apex:actionstatus>
                    <!-- Added by Vinay 05-05-2025 End -->
                    <apex:commandButton value="CANCEL" action="{!Cancel}"  immediate="true" />   
                </apex:pageBlockButtons>
                <!-----------------------------------Booking Details--------------------------------------------->
                <apex:pageBlockSection title="EOI Details" rendered="{!IF(eoRecord <> null,true,false)}">
                    <apex:OutputText id="eoiId" label="EOI">
                        <apex:outputLink value="/{!eoRecord.id}">{!eoRecord.Name}</apex:outputLink>
                    </apex:OutputText>
                    <apex:OutputText id="eoiStatus" value="{!eoRecord.RW_Status__c}" label="Status"/>
                </apex:pageBlockSection>
                
                <apex:pageBlockSection title="EOI Offers" rendered="{!NOT(ISBLANK(eoiOffers))}">
                    <apex:pageBlockTable value="{! eoiOffers }" var="eoioff" >
                        <apex:column value="{! eoioff.Offer__r.Name}"/>
                        <apex:column value="{! eoioff.Sub_Offer__r.Name}"/>
                    </apex:pageBlockTable>
                </apex:pageBlockSection>
                
                <apex:pageBlockSection title="Booking Details" >
                    <apex:outputText >Mandatory fields are marked with an asterisk (*) </apex:outputText>
                    <apex:outputText >&nbsp;</apex:outputText>
                    
                    <apex:inputField value="{!boookingObj.Opportunity__c}" label="Opportunity *" >
                        <apex:actionsupport event="onchange" action="{!getquotation}" Rerender="messages,selectquoteId,otherdetails,sourceOfBooking,SubsourceOfBooking,ApplicantPAddress,appincode,ApplicantDOB,,apcity,apMob,apEmail,apCntry"  status="status">
                            <apex:param name="optyidparam" assignTo="{!OptyId}" value="{!boookingObj.Opportunity__c}"  />
                        </apex:actionsupport>
                    </apex:inputField>
                    <apex:selectList id="selectquoteId" value="{!selectedQuote}" size="1" label="Quotation ">
                        <apex:selectOptions value="{!getQuote}"/> 
                        <apex:actionSupport event="onchange" action="{!getTokenAmount}"  status="status">
                            <apex:param name="selectedQuote" value="{!getQuote}" assignTo="{!selectedQuote}"/>
                        </apex:actionSupport>
                    </apex:selectList>
                    <apex:OutputText id="token" value="{!tokenamt}" label="Token Amount"/>
                    <apex:OutputText value="{!STamt}" label="GST On Token Amount"/> 
                    <!--  <apex:inputField value="{!boookingObj.Application_No__c}" label="Application Number"/>  * -->
                    
                    <apex:inputField id="futureCorrContact" value="{!boookingObj.Future_Correspondence_Contact__c}" label="Future Correspondence Contact *" >
                        <apex:actionSupport event="onchange"  status="status"/>
                    </apex:inputField>
                </apex:pageBlockSection>
                <apex:pageBlockSection id="futuredetail" Rendered="{!If(boookingObj.Future_Correspondence_Contact__c == 'Other',True,False)}" >
                    
                    <apex:inputField id="futureCorrContactRelation" value="{!boookingObj.Future_Correspondence_Contact_Relation__c}" label="Future Correspondence Contact Relation *"/>
                    <apex:inputField id="futureCorrContactName" value="{!boookingObj.Future_Correspondence_Contact_Name__c}" label="Future Correspondence Contact Name *"/>
                    <apex:inputField id="futureCorrContactPhone" value="{!boookingObj.Future_Correspondence_Contact_Phone__c}" label="Future Correspondence Contact Phone *" />
                    <apex:inputField id="futureCorrContactEmail" value="{!boookingObj.Future_Correspondence_Contact_Email__c}" label="Future Correspondence Contact Email *"/>
                    
                </apex:pageBlockSection>
                
