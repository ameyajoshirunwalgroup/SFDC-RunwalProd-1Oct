           function showDiv(thisselect, thisselect1) {
    var selected = thisselect.options[thisselect.selectedIndex].value;
    document.getElementById(thisselect1).style.display = "block";

    var i;
    for (i = 1; i <= 17; i++) {
        document.getElementById(thisselect1).rows[i].style.display = "block";
        document.getElementById(thisselect1).rows[i].setAttribute("style", "width:100%");
    }

    if (selected == 'Indian National') {
        for (i = 3; i <= 17; i++) { 
            document.getElementById(thisselect1).rows[i].style.display = "none";
        }
    }

    if (selected == 'Foreign Nationals Of Indian Origin') {
        for (i = 6; i <= 17; i++) { 
            document.getElementById(thisselect1).rows[i].style.display = "none";
        }
        document.getElementById(thisselect1).rows[1].style.display = "none";
        document.getElementById(thisselect1).rows[2].style.display = "none";
    }

    if (selected == 'For NRI') {
        for (i = 8; i <= 17; i++) { 
            document.getElementById(thisselect1).rows[i].style.display = "none";
        }
        document.getElementById(thisselect1).rows[2].style.display = "none";
         document.getElementById(thisselect1).rows[3].style.display = "none";
         document.getElementById(thisselect1).rows[4].style.display = "none";
    }

    if (selected == 'For Company') {
        for (i = 2; i <= 7; i++) {
            document.getElementById(thisselect1).rows[i].style.display = "none";
        }
        for (i = 15; i <= 17; i++) { 
            document.getElementById(thisselect1).rows[i].style.display = "none";
        }
    }

    if (selected == 'Partnership Firm') {
        for (i = 2; i <= 13; i++) {
            document.getElementById(thisselect1).rows[i].style.display = "none";
        }
    }

    if (selected == '') {
        for (i = 1; i <= 17; i++) { 
            document.getElementById(thisselect1).rows[i].style.display = "none";
        }
    }
}
 
