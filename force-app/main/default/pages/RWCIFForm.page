            $( document ).ready(function() {
               applyDatePicker();
                refreshValues();
            });

            function applyDatePicker()
            {
                $( ".clsDatePicker" ).datepicker({
                  dateFormat: "dd/mm/yy",
                  changeYear: true,
                  yearRange: "-50:+0"
                });
            }

            function refreshValues()
            {
                if($("[Id$='occuptn']").val() == 'Salaried')
                {
                    $("[Id$='service']").attr('checked', true);
                }
                else if($("[Id$='occuptn']").val() == 'Business')
                {
                    $("[Id$='bussn']").attr('checked', true);
                }
                else if($("[Id$='occuptn']").val() == 'Self Employed')
                {
                    $("[Id$='self']").attr('checked', true);
                }
                else if($("[Id$='occuptn']").val() == 'Others')
                {
                    $("[Id$='others']").attr('checked', true);
                }

                if($("[Id$='finalConfig']").val() == '1.5 BHK')
                {
                    $("[Id$='onefive']").attr('checked', true);
                }
                else if($("[Id$='finalConfig']").val() == '2 BHK')
                {
                    $("[Id$='twobhk']").attr('checked', true);
                }
                else if($("[Id$='finalConfig']").val() == '2.fivebhk')
                {
                    $("[Id$='secfbhk']").attr('checked', true);
                }
                else if($("[Id$='finalConfig']").val() == '3 BHK')
                {
                    $("[Id$='thrbhk']").attr('checked', true);
                }
                else if($("[Id$='finalConfig']").val() == '3.fivebhk')
                {
                    $("[Id$='threefbhk']").attr('checked', true);
                }
                else if($("[Id$='finalConfig']").val() == '4 BHK')
                {
                    $("[Id$='frbhk']").attr('checked', true);
                }
                else if($("[Id$='finalConfig']").val() == 'fivebhk')
                {
                    $("[Id$='fivebhk']").attr('checked', true);
                }
                else if($("[Id$='finalConfig']").val() == 'Retail')
                {
                    $("[Id$='retail']").attr('checked', true);
                }

                if($("[Id$='custType']").val() == 'NRI')
                {
                    $("[Id$='ResNRI']").attr('checked', true);
                    $("[Id$='ResNRI']").prop( "disabled", true );
                    $("[Id$='ResLoc']").prop( "disabled", true );
                    $("[Id$='ResOut']").prop( "disabled", true );
                }
                else if($("[Id$='custType']").val() == 'Local')
                {
                    $("[Id$='ResLoc']").attr('checked', true);
                }
                else if($("[Id$='custType']").val() == 'Outstation')
                {
                    $("[Id$='ResOut']").attr('checked', true);
                }
                if($("[Id$='timelineVal']").val() == 'Ready Possession')
                {
                    $("[Id$='readyPoss']").attr('checked', true);
                }
                else if($("[Id$='timelineVal']").val() == 'Near to Possession')
                {
                    $("[Id$='NearPos']").attr('checked', true);
                }
                else if($("[Id$='timelineVal']").val() == '1 to 2 year')
                {
                    $("[Id$='oneTotwoYr']").attr('checked', true);
                }
                else if($("[Id$='timelineVal']").val() == '2 to 3 years')
                {
                    $("[Id$='twoTothreeYr']").attr('checked', true);
                }
                else if($("[Id$='timelineVal']").val() == 'More than 3 years')
                {
                    $("[Id$='moreThanthre']").attr('checked', true);
                }
                else if($("[Id$='timelineVal']").val() == 'Not Important')
                {
                    $("[Id$='tlni']").attr('checked', true);
                }

                if($("[Id$='neighval']").val().indexOf('Schools') >= 0)
                {
                    $("[Id$='plschool']").attr('checked', true);
                }
                if($("[Id$='neighval']").val().indexOf('Creches') >= 0)
                {
                    $("[Id$='plscreches']").attr('checked', true);
                }
                if($("[Id$='neighval']").val().indexOf('Colleges') >= 0)
                {
                    $("[Id$='plsclg']").attr('checked', true);
                }
                if($("[Id$='neighval']").val().indexOf('Gardens') >= 0)
                {
                    $("[Id$='plsgard']").attr('checked', true);
                }
                if($("[Id$='neighval']").val().indexOf('Departmental Stores') >= 0)
                {
                    $("[Id$='plsdeprt']").attr('checked', true);
                }
                if($("[Id$='neighval']").val().indexOf('Malls') >= 0)
                {
                    $("[Id$='plsmall']").attr('checked', true);
                }
                if($("[Id$='neighval']").val().indexOf('Multiplexes') >= 0)
                {
                    $("[Id$='plsmulti']").attr('checked', true);
                }
                if($("[Id$='neighval']").val().indexOf('Hospitals') >= 0)
                {
                    $("[Id$='plshostp']").attr('checked', true);
                }
                if($("[Id$='neighval']").val().indexOf('Place of worship') >= 0)
                {
                    $("[Id$='plsworsh']").attr('checked', true);
                }
                if($("[Id$='neighval']").val().indexOf('Restaurants') >= 0)
                {
                    $("[Id$='plsrestr']").attr('checked', true);
                }
                if($("[Id$='neighval']").val().indexOf('Gyms') >= 0)
                {
                    $("[Id$='plsgyms']").attr('checked', true);
                }
                if($("[Id$='neighval']").val().indexOf('Night Clubs') >= 0)
                {
                    $("[Id$='plsnight']").attr('checked', true);
                }
                if($("[Id$='neighval']").val().indexOf('IT Parks') >= 0)
                {
                    $("[Id$='plsitp']").attr('checked', true);
                }
                if($("[Id$='neighval']").val().indexOf('High Street Shopping') >= 0)
                {
                    $("[Id$='plshigh']").attr('checked', true);
                }
            }
