                                <script>
                            function updateCustType(cntry)
                            {
                                if(cntry.indexOf('ResNRI') >= 0)
                                {
                                    if($("[Id$='ResNRI']").is(':checked'))
                                    {
                                        $("[Id$='ResLoc']").attr('checked', false);
                                        $("[Id$='ResOut']").attr('checked', false);
                                        $("[Id$='custType']").val('NRI');
                                    }
                                    else
                                    {
                                        $("[Id$='custType']").val('');
                                        //$("[Id$='resStatus']").hide();
                                    }
                                }
                                if(cntry.indexOf('ResLoc') >= 0)
                                {
                                    if($("[Id$='ResLoc']").is(':checked'))
                                    {
                                        $("[Id$='ResNRI']").attr('checked', false);
                                        $("[Id$='ResOut']").attr('checked', false);
                                        $("[Id$='custType']").val('Local');
                                    }
                                    else
                                    {
                                        $("[Id$='custType']").val('');
                                        //$("[Id$='resStatus']").hide();
                                    }
                                }

                                if(cntry.indexOf('ResOut') >= 0)
                                {
                                    if($("[Id$='ResOut']").is(':checked'))
                                    {
                                        $("[Id$='ResLoc']").attr('checked', false);
                                        $("[Id$='ResNRI']").attr('checked', false);
                                        $("[Id$='custType']").val('Outstation');
                                    }
                                    else
                                    {
                                        $("[Id$='custType']").val('');
                                    }
                                }
                            }
