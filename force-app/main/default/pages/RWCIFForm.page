                                <input type="checkbox" class="customechkBox" id="plsnight" onchange="updatePlaces(this.id)" /> Night Clubs
                            </label>
                        </div>
                        <div class="col-xs-6 col-md-3">
                            <label class="topPos_5">
                                <input type="checkbox" class="customechkBox" id="plsitp" onchange="updatePlaces(this.id)" /> IT Parks
                            </label>
                        </div>
                        <div class="col-xs-6 col-md-3">
                            <label class="topPos_5">
                                <input type="checkbox" class="customechkBox" id="plshigh" onchange="updatePlaces(this.id)" /> High Street Shopping
                            </label>
                        </div>
                        <apex:inputtext value="{!objLead.RW_Neighbourhood_Places__c}" style="display:none" id="neighval" />
                        <script>
                            function updatePlaces(ngVal)
                            {
                                $("[Id$='neighval']").val('');

                                if($("[Id$='plschool']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val('Schools;');
                                }
                                if($("[Id$='plscreches']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val($("[Id$='neighval']").val()+'Creches;');
                                }
                                if($("[Id$='plsclg']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val($("[Id$='neighval']").val()+'Colleges;');
                                }
                                if($("[Id$='plsgard']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val($("[Id$='neighval']").val()+'Gardens;');
                                }
                                if($("[Id$='plsdeprt']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val($("[Id$='neighval']").val()+'Departmental Stores;');
                                }
                                if($("[Id$='plsmall']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val($("[Id$='neighval']").val()+'Malls;');
                                }
                                if($("[Id$='plsmulti']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val($("[Id$='neighval']").val()+'Multiplexes;');
                                }
                                if($("[Id$='plshostp']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val($("[Id$='neighval']").val()+'Hospitals;');
                                }
                                if($("[Id$='plsworsh']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val($("[Id$='neighval']").val()+'Place of worship;');
                                }
                                if($("[Id$='plsrestr']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val($("[Id$='neighval']").val()+'Restaurants;');
                                }
                                if($("[Id$='plsgyms']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val($("[Id$='neighval']").val()+'Gyms;');
                                }
                                if($("[Id$='plsnight']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val($("[Id$='neighval']").val()+'Night Clubs;');
                                }
                                if($("[Id$='plsitp']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val($("[Id$='neighval']").val()+'IT Parks;');
                                }
                                if($("[Id$='plshigh']").is(':checked'))
                                {
                                    $("[Id$='neighval']").val($("[Id$='neighval']").val()+'High Street Shopping;');
                                }
                            }
                        </script>
                    </div>
                </div>
                <apex:outputpanel id="LeadSourcePanel" layout="block">
                    <div class="frmContainer">
                        <h2>How did you come to know about {!objLead.RW_Project__r.Name}? (Tick only one)</h2>
                        <!-- Lead Source -->
                        <div class="row">
                            <apex:repeat value="{!lstLeadSource}" var="EachSource">
                                <div class="col-xs-6 col-sm-4">
                                    <label class="topPos_5 form-group">
                                        <apex:inputcheckbox value="{!EachSource.isSelected}" styleclass="customechkBox">
                                            <apex:actionsupport action="{!validateLeadSource}" event="onchange" rerender="LeadSourcePanel,CPSignDiv" status="spinnerStatus">
                                                <apex:param name="CurrentSource" value="{!EachSource.strLeadSource}" assignto="{!strCurrentSource}" />
                                            </apex:actionsupport>
                                        </apex:inputcheckbox>
                                        {!EachSource.strLabel}
                                    </label>
                                </div>
                            </apex:repeat>
                        </div>
                    </div>
                    <div class="frmContainer">
                        <h2>Additional Informations</h2>
                        <!-- Lead Sub Source -->
                        <apex:outputpanel layout="block" styleclass="form-group" rendered="{!IF(objLead.LeadSource == 'Channel Partner', true, false)}">
                            <div class="row">
                                <div class="col-xs-6">
                                    <label class="topPos_5 form-group">
                                        <apex:inputcheckbox value="{!objLead.RW_Apply_Unregistered_Broker__c}" styleclass="customechkBox">
                                            <apex:actionsupport event="onchange" rerender="LeadSourcePanel" status="spinnerStatus">
                                            </apex:actionsupport>
                                        </apex:inputcheckbox>
                                        Apply Unregistered Broker
                                    </label>
                                </div>
                                <apex:outputpanel layout="block" styleclass="col-xs-6" rendered="{!!objLead.RW_Apply_Unregistered_Broker__c}">
                                    <label class="frmlabel">Channel Partner Name :</label>
                                    <div class="frmElem">
                                        <c:AutoCompleteV2 allowclear="true" importjquery="false" labelfield="Name" sobject="Broker__c" valuefield="Id"
                                                          targetfield="{!objLead.RW_Broker__c}" style="width:200px" syncmanualentry="false" />
                                    </div>
                                </apex:outputpanel>
                            </div>
                            <div class="row form-group">
                                <apex:outputpanel layout="block" styleclass="col-xs-6" rendered="{!objLead.RW_Apply_Unregistered_Broker__c}">
                                    <label class="frmlabel">
                                        Unregistered Broker Name :
                                    </label>
                                    <div class="frmElem">
                                        <apex:inputfield value="{!objLead.RW_Unregistered_Broker_Name__c}" styleclass="form-control" />
                                    </div>
                                </apex:outputpanel>
                                <apex:outputpanel layout="block" styleclass="col-xs-6" rendered="{!objLead.RW_Apply_Unregistered_Broker__c}">
                                    <label class="frmlabel">
                                        Unregistered Broker Email :
                                    </label>
                                    <div class="frmElem">
                                        <apex:inputfield value="{!objLead.RW_Unregistered_Broker_Email__c}" styleclass="form-control" />
                                    </div>
                                </apex:outputpanel>
                            </div>
                            <div class="row form-group">
                                <apex:outputpanel layout="block" styleclass="col-xs-6" rendered="{!objLead.RW_Apply_Unregistered_Broker__c}">
                                    <label class="frmlabel">
                                        Unregistered Broker Mobile :
                                    </label>
                                    <div class="frmElem">
                                        <apex:inputfield value="{!objLead.RW_Unregistered_Broker_Mobile__c}" styleclass="form-control" />
                                    </div>
                                </apex:outputpanel>
                            </div>
                        </apex:outputpanel>
                        <apex:outputpanel layout="block" styleclass="form-group" rendered="{!IF(objLead.LeadSource == 'Reference', true, false)}">
                            <div class="row">
                                <div class="col-xs-6">
                                    <label class="frmlabel">Reference Source :</label>
                                    <div class="frmElem addressBlk">
                                        <apex:inputfield value="{!objLead.LeadSource}" style="display:none" />
                                        <apex:inputfield value="{!objLead.RW_Reference_Source__c}" styleclass="form-control">
                                            <apex:actionsupport event="onchange" rerender="LeadSourcePanel" status="spinnerStatus">
                                            </apex:actionsupport>
                                        </apex:inputfield>
                                    </div>
                                </div>
                                <apex:outputpanel layout="block" styleclass="col-xs-6"
                                                  rendered="{!IF(objLead.RW_Reference_Source__c == 'Existing Customer Reference', true, false)}">
                                    <label class="frmlabel">Existing Customer Reference :</label>
                                    <div class="frmElem">
                                        <c:AutoCompleteV2 allowclear="true" importjquery="false" labelfield="Name" sobject="Account" valuefield="Id"
                                                          targetfield="{!objLead.Customer_Reference__c}" style="width:200px" syncmanualentry="false" />
                                    </div>
                                </apex:outputpanel>
                                <apex:outputpanel layout="block" styleclass="col-xs-6"
                                                  rendered="{!IF(OR(objLead.RW_Reference_Source__c == 'Management Reference', objLead.RW_Reference_Source__c == 'Employee Reference'), true, false)}">
                                    <label class="frmlabel">Referral Details :</label>
                                    <div class="frmElem">
                                        <apex:inputfield value="{!objLead.RW_Referral_Details__c}" styleclass="form-control" />
                                    </div>
                                </apex:outputpanel>
                            </div>
                        </apex:outputpanel>
                        <apex:outputpanel layout="block" styleclass="form-group"
                                          rendered="{!IF(AND(objLead.LeadSource != null, OR(objLead.LeadSource == 'Channel Partner',
                                          objLead.LeadSource == 'Reference', objLead.LeadSource == 'Corporate', objLead.LeadSource == 'Loyalty',
                                          objLead.LeadSource == 'Corporate(Unregistered)')), false, true)}">
                            <div class="row">
                                <div class="col-xs-6">
                                    <label class="frmlabel">Source :</label>
                                    <div class="frmElem addressBlk">
                                        <apex:inputfield value="{!objLead.LeadSource}" style="display:none" />
                                        <apex:inputfield value="{!objLead.RW_Lead_Sub_Source__c}" styleclass="form-control" />
                                    </div>
                                </div>
                            </div>
                        </apex:outputpanel>
                        <apex:outputpanel layout="block" styleclass="form-group"
                                          rendered="{!IF(OR(objLead.LeadSource == 'Corporate', objLead.LeadSource == 'Corporate(Unregistered)'), true, false)}">
                            <div class="row">
                                <div class="col-xs-6">
                                    <label class="frmlabel">Corporate Company :</label>
                                    <div class="frmElem addressBlk">
                                        <apex:inputfield value="{!objLead.LeadSource}" style="display:none" />
                                        <apex:inputfield value="{!objLead.Corporate_Company__c}" styleclass="form-control" />
                                    </div>
                                </div>
                                <div class="col-xs-6">
                                    <label class="frmlabel">Other Corporate Company :</label>
                                    <div class="frmElem addressBlk">
                                        <apex:inputfield value="{!objLead.NameofUnregisteredCorporate_LeadSource__c}" styleclass="form-control" />
                                    </div>
                                </div>
                            </div>
                        </apex:outputpanel>
                        <!--<apex:outputpanel layout="block" styleclass="form-group"
                                          rendered="{!IF(objLead.LeadSource == 'Corporate(Unregistered)', true, false)}">
                            <div class="row">
                                <div class="col-xs-6">
                                    <label class="frmlabel">Other Corporate Company :</label>
                                    <div class="frmElem addressBlk">
                                        <apex:inputfield value="{!objLead.NameofUnregisteredCorporate_LeadSource__c}" styleclass="form-control" />
                                    </div>
                                </div>
                            </div>
                        </apex:outputpanel>-->
                        <apex:outputpanel layout="block" styleclass="form-group"
                                          rendered="{!IF(objLead.LeadSource == 'Loyalty', true, false)}">
                            <div class="row">
                                <div class="col-xs-6">
                                    <label class="frmlabel">Loyalty :</label>
                                    <div class="frmElem addressBlk">
                                        <c:AutoCompleteV2 allowclear="true" importjquery="false" labelfield="Name" sobject="Account" valuefield="Id"
                                                          targetfield="{!objLead.Customer_Reference__c}" style="width:200px" syncmanualentry="false" />
                                    </div>
                                </div>
                            </div>
                        </apex:outputpanel>
                    </div>
                </apex:outputpanel>
                <div class="row mrgn_t45 hidden-md hidden-lg" id="CPSignDiv">
                    <div class="col-xs-7 col-sm-6">
                        <c:RWSignatureComponent parentid="{!objLead.id}" attname="CustomerSign" />
                        <h2>Customer's Signature</h2>
                    </div>
                    <div class="col-xs-7 col-sm-6">
                        <c:RWSignatureComponent parentid="{!objLead.id}" attname="CPSign" />
                        <h2>Channel Partner's Signature</h2>
                    </div>
                </div>
                <div class="col-xs-12" style="margin-bottom: 15px;">
                    <!--<apex:commandbutton value="Save & Exit" styleclass="pull-right btn btn-success pull-right" action="{!redirectNext}" rerender="errorDiv"
                                        status="spinnerStatus" />
                    <apex:commandbutton styleclass="btn btn-danger pull-left" value="Back" action="{!GoBackToLead}" status="spinnerStatus" />-->

                    <apex:commandbutton value="Save Form & Exit" styleclass="pull-right btn-custm btn-success-custm pull-right" action="{!redirectNext}" 
                                        rendered="{!AND($User.UIThemeDisplayed !='Theme4d',$User.UIThemeDisplayed !='Theme4t')}" />
                    <apex:commandbutton action="{!redirectNext}" value="Save & Close" oncomplete="redirectTo({!isSubmitted});"
                                        styleclass="pull-right btn-custm btn-success-custm"
                                        rendered="{!OR($User.UIThemeDisplayed == 'Theme4d',$User.UIThemeDisplayed == 'Theme4t')}" />
                    <script>
                    function redirectTo(isSubmitted)
                    {
                        if((typeof sforce.one != 'undefined') && (sforce.one != null))
                        {
                            if(isSubmitted)
                            {
                                sforce.one.navigateToSObject('{!objLead.id}');
                            }
                            else 
                            {
                                refreshError();
                            }
                        }
                    }
                    </script>
                    
                    <apex:commandbutton styleclass="pull-left btn-custm btn-danger-custm" value="< Back" action="{!GoBackToLead}" 
                                        rendered="{!AND($User.UIThemeDisplayed !='Theme4d',$User.UIThemeDisplayed !='Theme4t')}" />
                    <apex:commandbutton onclick="cancel_JS()" value="< Back" styleclass="pull-left btn-custm btn-danger-custm" 
                                        rendered="{!OR($User.UIThemeDisplayed == 'Theme4d',$User.UIThemeDisplayed == 'Theme4t')}" />
                    <script>
                    function cancel_JS()
                    {
                        if((typeof sforce.one != 'undefined') && (sforce.one != null))
                            sforce.one.navigateToSObject('{!objLead.id}');
                    }
                    </script>
                    <apex:actionfunction name="refreshError" status="spinnerStatus" rerender="errorDiv" />
                </div>
            </apex:form>
        </div>
    </body>
</html>
</apex:page>