                           style="font-family:Arial MT; border-collapse:collapse; border: 1px solid #000; width: 100%; table-layout: fixed;">
                        <thead>
                            <tr style="background-color: #fff;">
                                <th style="font-size:13px; border: 1px solid #000;"></th>
                                <th style="font-size:13px; border: 1px solid #000; text-align: center;">1<sup>st</sup> Applicant</th>
                                <th style="font-size:13px; border: 1px solid #000; text-align: center;" rendered="{!showSecond}">2<sup>nd</sup> Applicant</th>
                                <th style="font-size:13px; border: 1px solid #000; text-align: center;" rendered="{!showThird}">3<sup>rd</sup> Applicant</th>
                            </tr>
                        </thead>
                        
