                                <td rendered="{!showSecond}" style="border: 1px solid #000; font-size: 13px; text-align: center;">{!second_Applicant_Wrapper.isResedentialStatus}</td>
                                <td rendered="{!showThird}" style="border: 1px solid #000; font-size: 13px; text-align: center;">{!third_Applicant_Wrapper.isResedentialStatus}</td>
                            </tr>
                            <tr>
                                <td style="border: 1px solid #000; font-size: 13px;">PAN No.</td>
                                <td style="border: 1px solid #000; font-size: 13px; text-align: center;">{!first_Applicant_Wrapper.isPanNo}</td>
                                <td rendered="{!showSecond}" style="border: 1px solid #000; font-size: 13px; text-align: center;">{!second_Applicant_Wrapper.isPanNo}</td>
                                <td rendered="{!showThird}" style="border: 1px solid #000; font-size: 13px; text-align: center;">{!third_Applicant_Wrapper.isPanNo}</td>
                            </tr>
