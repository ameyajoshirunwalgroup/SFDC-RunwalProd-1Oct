        <head>
            <style>
                .signature-box {
                width: 100px;
                height: 120px;
                border: 1px solid #000;
                display: inline-block;
                margin: 0 64px;
                align: left;
                }
                .signature-label {
                text-align: center;
                font-weight: bold;
                margin-top: 5px;
                margin: 0 64px;
                }
                .signature-title {
                text-align: center;
                margin-top: 5px;
                margin: 0 64px;
                }
                
                table.signature-table {
                border-collapse: collapse;
                margin-top: 30px;
                width: 700px;
                }
                
                table.signature-table td {
                border: 1px solid black;
                text-align: center;
                padding: 20px;
                }
                
                .custom-checkbox {
                display: inline-block;
                width: 10px;
                height: 10px;
                border: 1px solid #000;
                margin-right: 5px;
                vertical-align: center;
                }
                
                @page{
                size: A4 potrait;
                margin: 0;
                padding: 0;
                }
                
                .custom {
                margin: 50px;
                } 
                
                 .customPage {
                margin-Top: 30px;
                margin-Bottom: 30px;
                margin-left: 20px;
                } 
            </style>
        </head>
        
        
        <body>
            <!-- Page 1 start -->
           <div>
                <img src="{!FrontPageofProject}" style="position: absolute; top: 0; left: 0; margin: 0; padding: 0; width: 216mm; height: 279.5mm;
                overflow: hidden;"/>
               
            </div>
            <!-- Page 1 end -->
            
            <div style="page-break-before:always;"></div>
            <div>
            <apex:outputPanel rendered="{!showPriceSheet}">
