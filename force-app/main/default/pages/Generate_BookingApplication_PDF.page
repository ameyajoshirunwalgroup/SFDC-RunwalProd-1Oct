                                <td style="border: 1px solid #000; font-size: 13px;">Aadhar Card No.</td>
                                <td style="border: 1px solid #000; font-size: 13px; text-align: center;">{!first_Applicant_Wrapper.isAadharNo}</td>
                                <td rendered="{!showSecond}" style="border: 1px solid #000; font-size: 13px; text-align: center;">{!second_Applicant_Wrapper.isAadharNo}</td>
                                <td rendered="{!showThird}" style="border: 1px solid #000; font-size: 13px; text-align: center;">{!third_Applicant_Wrapper.isAadharNo}</td>
                            </tr>
                            <tr>
                                <td style="border: 1px solid #000; font-size: 13px;">GSTN No.</td>
                                <td style="border: 1px solid #000; font-size: 13px; text-align: center;">{!first_Applicant_Wrapper.isGstNo}</td>
                                <td rendered="{!showSecond}" style="border: 1px solid #000; font-size: 13px; text-align: center;">{!second_Applicant_Wrapper.isGstNo}</td>
                                <td rendered="{!showThird}" style="border: 1px solid #000; font-size: 13px; text-align: center;">{!third_Applicant_Wrapper.isGstNo}</td>
                            </tr>
                            <tr>
                                <td style="border: 1px solid #000; font-size: 13px;">Permanent Address</td>
                                <td style="border: 1px solid #000; font-size: 13px; text-align: center;">{!first_Applicant_Wrapper.isPerAddress}</td>
                                <td rendered="{!showSecond}" style="border: 1px solid #000; font-size: 13px; text-align: center;">{!second_Applicant_Wrapper.isPerAddress}</td>
                                <td rendered="{!showThird}" style="border: 1px solid #000; font-size: 13px; text-align: center;">{!third_Applicant_Wrapper.isPerAddress}</td>
                            </tr>
                            <tr>
                                <td style="border: 1px solid #000; font-size: 13px;">Address for Communication</td>
                                <td style="border: 1px solid #000; font-size: 13px; text-align: center;">{!first_Applicant_Wrapper.isCommunAddress}</td>
                                <td rendered="{!showSecond}" style="border: 1px solid #000; font-size: 13px; text-align: center;">{!second_Applicant_Wrapper.isCommunAddress}</td>
                                <td rendered="{!showThird}" style="border: 1px solid #000; font-size: 13px; text-align: center;">{!third_Applicant_Wrapper.isCommunAddress}</td>
                            </tr>
