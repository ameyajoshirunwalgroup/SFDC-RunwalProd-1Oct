                <script>
                function numericSixDigit(inputField) {
                    const value = inputField.value.trim();
                    const errorDiv = document.getElementById("sixDigitIndianZipError");
                    
                    // Clear previous error
                    errorDiv.innerText = "";
                    
                    if (!/^\d{6}$/.test(value)) {
                        errorDiv.innerText = "Please enter a valid 6-digit number.";
                        inputField.focus();
                    }
                }
                </script>
